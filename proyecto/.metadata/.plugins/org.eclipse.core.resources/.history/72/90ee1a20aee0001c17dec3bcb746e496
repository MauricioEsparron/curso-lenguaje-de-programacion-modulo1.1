package utils;

public class Validaciones {

	public static final String CODIGO_CLIENTE = "[0-9]d{4}";
	public static final String DNI = "\\d{8}";

	public static final String CODIGO_PRODUCTO = "[Pp]\\d{4}";

	public static final String CODIGO_CLIENTES = "[CL]\\d{3}";

	public static final String TEXTO = "[a-zA-ZZáéíóúÁÉÍÓÚ\\s]{3,15}";
	public static final String NOMBRE = "[a-zA-ZáéíóúÁÉÍÓÚ\\s]{3,30}";

	public static final String APELLIDO = "[a-zA-ZáéíóúÁÉÍÓÚ\\s]{3,30}";

	public static final String TELEFONO = "\\d{9}";

}
