package hilos;

import vista.FrmPreLoader;

public class HiloBarraProgreso extends Thread {
	@Override
	public void run() {
		for (int i = 0; i <= 100; i++) {
			FrmPreLoader.prbCarga.setValue(i);
			//proceso --> pausa
			
			try {
				Thread.sleep(50);
				
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
		}
	}

}
