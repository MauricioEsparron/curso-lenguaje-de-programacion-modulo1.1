		fecAfiliacion = getFechaAfiliacion();
		fecCaducidad = getFechaCaducidad();
		saldo = getSaldo();

		if (numTarjeta == -1 || codCliente == 0 || codCuenta == 0 || numCuenta == -1 || fecAfiliacion == null
				|| fecCaducidad == null || saldo == -1) {
			return;
