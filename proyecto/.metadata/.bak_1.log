		}
		try {
			txtFechaCaducidad.setDate(new SimpleDateFormat("yyyy-MM-dd").parse(fechaCaducidad));
		} catch (ParseException e) {
			e.printStackTrace();
		}
		txtSaldo.setText(saldo);

	}

