	private double getSaldo() {
		double saldo = -1;
		if (txtSaldo.getText().trim().length() == 0) {
			mensajeError("Por favor ingresa el saldo del Cliente");
			txtSaldo.requestFocus();
			txtSaldo.setText("");
			txtSaldo.setBackground(Color.RED);
