			t.setCod_cuenta(codTipoCuenta);
			t.setNum_cuenta(numCuenta);
			t.setFecAfliacion(fecAfiliacion);
			t.setFecCaducidad(fecCaducidad);

			int ok = gTarjeta.actualizar(t);
