	private void mostraDataTarjeta() {

		model.setRowCount(0);
		ArrayList<Tarjeta> data = gTarjeta.listartarjeta();
		for (Tarjeta t : data) {
			Object fila[] = { t.getNum_Tarjeta(), t.getCod_cliente(), t.getCod_cuenta(), t.getNum_cuenta(),
					t.getFecAfliacion(), t.getFecCaducidad(), t.getSaldo() };
			model.addRow(fila);
		}

	}

